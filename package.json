{
  "name": "portfolio-redesigned",
  "version": "0.1.0",
  "private": true,
  "pnpm": {
    "version": "7.0.0-rc.9"
  },
  "engines": {
    "node": "17.4.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.14",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "express": "^4.18.2",
    "pdfjs-dist": "^3.4.120",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^6.2.2",
    "react-router-dom": "^6.9.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-local": "concurrently \"pnpm vite-dev\" \"pnpm tailwind-watch\"",
    "tailwind-watch": "tailwindcss -i ./src/index.css -o ./public/output.css --watch",
    "vite-dev": "vite",
    "build-prod": "tsc && vite build",
    "build-tailwind-prod": "tailwindcss -i ./src/index.css -o ./public/output.css",
    "deploy": "pnpm build-tailwind-prod && pnpm build-prod && node server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-pdf": "^6.2.0",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^8.0.1",
    "tailwindcss": "^3.2.7",
    "vite": "^4.2.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.7"
  }
}